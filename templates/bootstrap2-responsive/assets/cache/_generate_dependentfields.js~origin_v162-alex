;$(document).ready(function(){$(".search-form form select[name='option3_1'], .search-form form #search_option_3,.search-form form input[rel][name='option3_1']").change(function(){var n=$(this).children("option:selected").index(),o=$(this).parent().parent(),i=$(this).parent(),t=$(this).attr("rel");if(typeof t!==typeof undefined&&t!==!1){n=t;o=o.parent();i=i.parent()};$(".dependenthide").removeClass("dependenthide");if(typeof t!==typeof undefined&&t!==!1){$(this).parent().parent().find("select").each(function(){if($(this).val()!=""){e(o,i,$(this).val())}})}else{e(o,i,n)}});$(".search-form form #search_option_3.tabbed-selector li a").click(function(){var t=$(this).parent().attr("class").replace("cat_","").replace("active","");t=$.trim(t);var o="",i="";$(".dependenthide").removeClass("dependenthide");if(typeof index_tree!==undefined){e(o,i,t)}});function e(i,t,e){if(e=="0"){$("[data-id='search_option__from'],[data-id='search_option__to'],[data-id='search_option_'],#search_option__from,#search_option__to,#search_option_,[data-option_id='']").not(".skip-input").each(function(){if(this.type=="text"||this.type=="textarea"){this.value=""}else if(this.type=="radio"||this.type=="checkbox"){this.checked=!1}else if(this.type=="select-one"||this.type=="select-multiple"){this.value="";if(this.value!="")this.value="-"}else if(this.type=="hidden"){this.value=""}else if($(this).hasClass("dropdown-toggle")){}});$("[data-id='search_option__from'],[data-id='search_option__to'],[data-id='search_option_'],#search_option__from,#search_option__to,#search_option_,[data-option_id='']").each(function(){if(this.type=="text"||this.type=="textarea"){$(this).not("form").addClass("dependenthide")}else if(this.type=="radio"||this.type=="checkbox"){$(this).parent().not("form").addClass("dependenthide")}else if(this.type=="select-one"||this.type=="select-multiple"){$(this).not("form").addClass("dependenthide")}else if(this.type=="hidden"){$(this).not("form").addClass("dependenthide")}else if($(this).hasClass("dropdown-toggle")){$(this).parent().not("form").addClass("dependenthide")}});if($("form.secondary-form div:not(.dependenthide) input[data-option_id]").length==0){$("form.secondary-form").parent().parent().addClass("dependenthide")}}};$(".search-form .TREE-GENERATOR select").trigger("change")});